#include<iostream>
using namespace std;

/*
3. Да се напише функция sort3(int& min, int& mid, int& max), която приема 3 числа и ги сортира във възходящ ред.

Вход: 4 5 3 | Изход: 3 4 5
*/



void InsertionSort(int* arr, int length) {

	for (int i = 1; i < length; i++)
	{
		int temp = arr[i];
		int j = i - 1;

		while (j >= 0 && arr[j] > temp) {
			arr[j + 1] = arr[j];
			j = j - 1;
		}
		arr[j + 1] = temp;
	}

}

void sort3(int& min, int& mid, int& max) {

	int arr[3];
	arr[0] = min;
	arr[1] = mid;
	arr[2] = max;

	InsertionSort(arr, 3);

	min = arr[0];
	mid = arr[1];
	max = arr[2];

	
	
}



int main()
{
	int min = 6;
	int max = 9;
	int mid= 1234234;


	sort3(min, mid, max);
	
	cout << "MIN " << min << " MID " << mid << " MAX " << max;

}
