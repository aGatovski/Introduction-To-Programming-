#include <iostream>
using namespace std;

void swap(int& a, int& b) {
	int c = a;
	a = b;
	b = c;
}

int GCD(int a, int b) {
	if (a < b) swap(a, b);

	while (b!=0) {
		int temp = b;
		b = a % b;
		a = temp;
	}
	return a;
}

void reduceFraction(int& numerator, int& denominator) {

	int devisor = GCD(numerator, denominator);
	while (devisor > 1 && devisor != numerator && devisor!= denominator) {
		numerator /= devisor;
		denominator /= devisor;
		devisor = GCD(numerator, denominator);
	}

}

void sumFractions(int& numerator1, int& denominator1, int& numerator2, int& denominator2) {
	
	int sumNumerator = (numerator1*denominator2) + (numerator2*denominator1);
	int sumDenominator = denominator2*denominator1;
	reduceFraction(sumNumerator, sumDenominator);
	cout << sumNumerator << " " << sumDenominator;
}

int main() {
	int arr[4];
	for (int i = 0; i < 4; i++)
	{
		cin >> arr[i];
	}
	sumFractions(arr[0], arr[1], arr[2], arr[3]);
}
