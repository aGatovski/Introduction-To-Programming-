#include <iostream>

int main()
{
    const int length=1000;
 
	int arr[length];
	int N;
	std::cin >> N;
	for (int i = 0; i < N; i++)
	{
		int a;
		std::cin >> a;

		arr[i] = a;

	}
	for (int i = 0; i < N; i++)
	{
		for (int j = 0; j < N; j++)
		{
			if (arr[i] == arr[j]) 
			{ 
			std::cout << arr[i] << " is the first repeated number!"; 
			return 0;
			}
		}
	}

}

