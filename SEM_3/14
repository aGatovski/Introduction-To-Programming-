#include <iostream>

int main()
{
    int n;
    std::cin >> n;
    if (n == 1)
    {
        std::cout << "true";
        return 0;
    }
    while (n >=2)
    {
       
        if (n % 2 != 0) 
        {
            std::cout << "false";
            return 0;
        }
        n /= 2;
    }
    std::cout << "true";
    
}
